{
  "name": "@project/web-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start -p 4000",
    "build": "next build",
    "pages:build": "npm run build-cf-types && wrangler -c /apps/web-next && npx @cloudflare/next-on-pages",
    "preview": "npm run pages:build && wrangler pages dev .vercel/output/static",
    "deploy": "npm run pages:build",
    "build-cf-types": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@meilisearch/instant-meilisearch": "^0.14.0",
    "@project/common": "*",
    "@project/ui": "*",
    "@vercel/analytics": "^1.0.1",
    "framer-motion": "^10.12.18",
    "js-cookie": "^3.0.5",
    "nanoid": "^5.0.4",
    "next": "14.2.2",
    "next-auth": "4.24.7",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-instantsearch-dom": "^6.40.4",
    "sharp": "^0.33.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.11.0",
    "@cloudflare/workers-types": "4.20240405.0",
    "@next/eslint-plugin-next": "^13.4.19",
    "@project/eslint-config": "*",
    "@project/tailwind-config": "*",
    "@project/ts-config": "*",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.11.17",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "wrangler": "3.51.0"
  }
}
