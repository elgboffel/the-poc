{
  "extends": "astro/tsconfigs/strict.json",
  "compilerOptions": {
    "moduleResolution": "Node16",
    "target": "ES2020",
    "module": "ES2022",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@config/*": ["src/config/*"]
    }
  },
}
